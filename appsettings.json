{
  "Kafka": {
    "Brokers": "localhost:9092"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace"
    }
  },
  "LogConfiguration": {
    "factoryAdapter": {
      "type": "Common.Logging.NLog.NLogLoggerFactoryAdapter, Common.Logging.NLog20",
      "arguments": {
        "configType": "FILE",
        "configFile": "~/nlog.config"
      }
    }
  },
  "AllowedHosts": "*"
}